global:
    scrape_interval: 10s
scrape_configs:
    - job_name: prometheus
      static_configs:
        - targets:
          - localhost:{{ prometheus_port|default('9090') }}
    - job_name: metric_exporter
      static_configs:
        - targets:
            - {{ metric_exporter_host|default('metric_exporter') }}:{{ metric_exporter_port|default('9095') }}
